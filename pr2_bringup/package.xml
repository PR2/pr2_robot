  <run_depend>sound_play</run_depend>
  <run_depend>joy</run_depend>
  <run_depend>std_srvs</run_depend>
  <run_depend>pr2_run_stop_auto_restart</run_depend>
  <run_depend>rosbag</run_depend>
  <run_depend>pr2_power_board</run_depend>
  <run_depend>ocean_battery_driver</run_depend>
  <run_depend>power_monitor</run_depend>
  <run_depend>pr2_computer_monitor</run_depend>
  <run_depend>diagnostic_aggregator</run_depend>
  <run_depend>pr2_dashboard_aggregator</run_depend>
